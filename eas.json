{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {}
    },
    "production": {
      "distribution": "store",
      "env": {
        "APP_ENV": "production",
        "EXPO_PUBLIC_API_URL": "<YOUR_PROD_API_URL_FROM_GCP_CLOUD_RUN>",
        "EXPO_PUBLIC_SUPABASE_URL": "<YOUR_PROD_SUPABASE_URL>",
        "EXPO_PUBLIC_SUPABASE_PUB_KEY": "<YOUR_PROD_SUPABASE_PUBLISHABLE_KEY>",
        "EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID": "<YOUR_IOS_CLIENT_ID_FROM_GOOGLE_CONSOLE>",
        "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "<YOUR_WEB_CLIENT_ID_FROM_GOOGLE_CONSOLE>"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      }
    }
  }
}